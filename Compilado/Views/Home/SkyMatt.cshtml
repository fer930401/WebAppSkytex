
@{
    ViewBag.Title = "SkyMatt";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<ul class="list-inline">
    <li><img src="/Media/skytex.png" width="190" height="90" /></li>
</ul>
<br />
