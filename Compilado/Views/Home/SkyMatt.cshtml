    <div class="row">
        <div class="well well-lg">
            <h3>@ViewBag.Title <small>Skytex Mexico S.A de C.V.</small></h3>
        </div>
    </div>
</div>
<br />
<div class="row">
    <div class="col-xs-6 col-md-3">
        <a class="thumbnail">
            <img src="/Media/Skymatt2.png" width="190" height="90" />
        </a>
    </div>
    <p class="lead text-justify">
        @Skytex.Recursos.Textos.strParrafoSkyMatt1
    </p>
    <p class="lead text-justify">
        @Skytex.Recursos.Textos.strParrafoSkyMatt2
    </p>
    <p class="lead text-justify">
        @Skytex.Recursos.Textos.strParrafoSkyMatt3
    </p>
</div>
<br />
<address>
    <strong>Rafael Sagahón Ramos</strong><br>
    <abbr>@Skytex.Recursos.Textos.strParrafoSkyMatt4</abbr>
